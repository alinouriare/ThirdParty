@model List<Person>

@{
    ViewData["Title"] = "Home Page";
}

<table>

    <tr>
        <th>ID</th>

        <th>FirstName</th>

        <th>LastName</th>



    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
        </tr>
    }
</table>